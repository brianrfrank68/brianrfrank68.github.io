flb_team_init,
Batting.Name,
       Batting.Age,
       Batting.Tm,
       Batting.G,
       Batting.AB,
       Batting.R,
       Batting.H,
       Batting.&quot;2B&quot;,
       Batting.&quot;3B&quot;,
       Batting.HR,
       Batting.RBI,
       Batting.SB,
       Batting.CS,
       Batting.BB,
       Batting.SO,
       Batting.BA,
       Batting.OBP,
       Batting.SLG,
       Batting.OPS,
       Batting.&quot;Pos Summary&quot;,
       Batting.&quot;Name-additional&quot;,
	   MainPos AS Position
  FROM Batting 
LEFT OUTER JOIN roster on Batting.&quot;Name-additional&quot; = roster.player_id
LEFT OUTER JOIN player_list on player_list.player_id = Batting.&quot;Name-additional&quot;
WHERE 
roster.flb_team_init = 'WAIVE'
ORDER BY flb_team_init, Batting.&quot;Name-additional&quot;</sql><sql name="Pitching">
