row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort><column index="5" mode="1"/></sort><column_widths><column index="1" value="178"/><column index="2" value="98"/><column index="3" value="150"/><column index="4" value="82"/><column index="5" value="122"/><column index="6" value="96"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="roster" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort><column index="2" mode="0"/></sort><column_widths><column index="1" value="221"/><column index="2" value="126"/><column index="3" value="97"/></column_widths><filter_values><column index="1" value="Sanchez"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Batting">SELECT 
--flb_team_init,
Batting.Player,
       Batting.Age,
       Batting.Team,
       Batting.G,
       Batting.AB,
       Batting.R,
       Batting.H,
       Batting.&quot;2B&quot;,
       Batting.&quot;3B&quot;,
       Batting.HR,
       Batting.RBI,
       Batting.SB,
       Batting.CS,
       Batting.BB,
       Batting.SO,
       Batting.BA,
       Batting.OBP,
       Batting.SLG,
       Batting.OPS,
       Batting.&quot;Pos&quot;,
       Batting.&quot;Player-additional&quot;,
	   MainPos AS Position
  FROM Batting_2024 AS Batting
LEFT OUTER JOIN roster on Batting.&quot;Player-additional&quot; = roster.player_id
LEFT OUTER JOIN player_list on player_list.player_id = Batting.&quot;Player-additional&quot;
WHERE 
roster.flb_team_init IS NULL
ORDER BY player_list.MainPosNum, &quot;Player-additional&quot;</sql><sql name="Pitching">
SELECT 
--flb_team_init,
pitching.Player,
       Age,
       Team,
       W,
       L,
       ERA,
       G,
       GS,
       GF,
       SV,
       IP,
       H,
       R,
       ER,
       HR,
       BB,
       SO,
       FIP,
       WHIP,
       H9,
       HR9,
       BB9,
       SO9,
       &quot;SO/BB&quot;,
       pitching.&quot;Player-additional&quot;,
	   MainPos AS Position
  FROM pitching_2024 AS Pitching
LEFT OUTER JOIN roster on pitching.&quot;Player-additional&quot; = roster.player_id
LEFT OUTER JOIN player_list on player_list.player_id = pitching.&quot;Player-additional&quot;
  --inner join fielding on fielding.player_id = pitchers.player_id
 WHERE
  roster.flb_team_init IS NULL
  ORDER BY player_list.MainPosNum DESC, Pitching.&quot;Player-additional&quot;</sql><sql name="Positional Ranking">SELECT Batting.Name,
--roster.flb_team_init,
       Batting.Age,
       Dynasty_March_2024.Rank,
--       Dynasty_October.Prospect,
       Dynasty_March_2024.&quot;POS.&quot;,
       Dynasty_March_2024.&quot;SEC.POS.&quot;,
       Dynasty_Prospect_March_2024.Rank,
       Dynasty_Prospect_March_2024.&quot;POS.&quot;,
       Dynasty_Prospect_March_2024.&quot;SEC.POS.&quot;,
       Dynasty_Prospect_March.Rank,
       Dynasty_Prospect_March.&quot;POS.&quot;,
       Dynasty_Prospect_March.&quot;SEC. POS.&quot;,
       Batting.Tm,
       Batting_Value.WAR,
       Batting.G,
       Batting.AB,
       Batting.R,
       Batting.H,
       Batting.&quot;2B&quot;,
       Batting.&quot;3B&quot;,
       Batting.HR,
       Batting.RBI,
       Batting.SB,
       Batting.CS,
       Batting.BB,
       Batting.SO,
       Batting.BA,
       Batting.OBP,
       Batting.SLG,
       Batting.OPS,
       Batting.&quot;Pos Summary&quot;,
       --Fielding.&quot;Pos Summary&quot;,
       Batting.&quot;Name-additional&quot;,
	   MainPos
  FROM Batting 
LEFT OUTER JOIN roster on Batting.&quot;Name-additional&quot; = roster.player_id
LEFT OUTER JOIN Batting_Value on Batting.&quot;Name-additional&quot; = Batting_Value.&quot;Name-additional&quot;
LEFT OUTER JOIN player_list on player_list.player_id = Batting.&quot;Name-additional&quot;
LEFT OUTER JOIN Dynasty_March_2024 on (player_list.player_name = Dynasty_March_2024.Player OR Dynasty_March_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Dynasty_Prospect_March_2024 on (player_list.player_name = Dynasty_Prospect_March_2024.Player OR Dynasty_Prospect_March_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Dynasty_Prospect_March on (player_list.player_name = Dynasty_Prospect_March.Player OR Dynasty_Prospect_March.Player = player_list.alt_name)
WHERE 
(
Batting.&quot;Pos Summary&quot; LIKE '%'
--OR Batting.&quot;Pos Summary&quot; LIKE '*7%'
--OR Batting.&quot;Pos Summary&quot; LIKE '/7%'
)
AND roster.flb_team_init = 'WAIVE'
--AND (CAST(Batting.Age AS INT) &lt; 28 OR CAST(Batting_Value.WAR AS NUMERIC) &gt; 0)
ORDER BY 
CAST(Dynasty_March_2024.Rank AS INT) ASC NULLS LAST, 
CAST(Dynasty_Prospect_March_2024.Rank AS INT) ASC NULLS LAST, 
CAST(Dynasty_Prospect_March.Rank AS INT) ASC NULLS LAST, 
Batting_Value.WAR DESC, Batting.&quot;Name-additional&quot;</sql><sql name="All Batter Rankings">SELECT Batting_2024.Player,
roster.flb_team_init,
Batting_2024.&quot;Pos&quot;,
       Batting_2024.OPS,
	   Batting_RHP_2024.OPS AS OPS_R,
	   Batting_LHP_2024.OPS AS OPS_L,
       Batting_2024.Age,
       Dynasty_November_2024.Rank,
       Dynasty_November_2024.&quot;POS.&quot;,
       Dynasty_November_2024.&quot;SEC.POS.&quot;,
       December_Prospect_2024.Rank,
       December_Prospect_2024.&quot;POS.&quot;,
       December_Prospect_2024.&quot;SEC.POS.&quot;,
       Batting_2024.Team,
       Batting_Value_2024.WAR,
       Batting_2024.G,
       Batting_2024.AB,
       Batting_2024.R,
       Batting_2024.H,
       Batting_2024.&quot;2B&quot;,
       Batting_2024.&quot;3B&quot;,
       Batting_2024.HR,
       Batting_2024.RBI,
       Batting_2024.SB,
       Batting_2024.CS,
       Batting_2024.BB,
       Batting_2024.SO,
       Batting_2024.BA,
       Batting_2024.OBP,
       Batting_2024.SLG,
       Batting_2024.OPS,
       Batting_2024.&quot;Pos&quot;,
       --Fielding.&quot;Pos Summary&quot;,
       Batting_2024.&quot;Player-additional&quot;,
	   MainPos
  FROM Batting_2024 
LEFT OUTER JOIN roster on Batting_2024.&quot;Player-additional&quot; = roster.player_id
LEFT OUTER JOIN Batting_Value_2024 on Batting_2024.&quot;Player-additional&quot; = Batting_Value_2024.&quot;Player-additional&quot;
LEFT OUTER JOIN player_list on player_list.player_id = Batting_2024.&quot;Player-additional&quot;
LEFT OUTER JOIN Dynasty_November_2024 on (player_list.player_name = Dynasty_November_2024.Player OR Dynasty_November_2024.Player = player_list.alt_name)
LEFT OUTER JOIN December_Prospect_2024 on (player_list.player_name = December_Prospect_2024.Player OR December_Prospect_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Batting_RHP_2024 on Batting_RHP_2024.&quot;Player-additional&quot; = Batting_2024.&quot;Player-additional&quot;
LEFT OUTER JOIN Batting_LHP_2024 on Batting_LHP_2024.&quot;Player-additional&quot; = Batting_2024.&quot;Player-additional&quot;
WHERE 
(
--Batting_2024.&quot;Pos&quot; LIKE '%3%' 
Batting_2024.&quot;Pos&quot; LIKE '%4%'
OR Batting_2024.&quot;Pos&quot; LIKE '*4%'
OR Batting_2024.&quot;Pos&quot; LIKE '/4%'
)
AND (roster.flb_team_init IS NULL OR roster.flb_team_init = 'BFP')
--AND (CAST(Batting_2024.Age AS INT) &lt; 28 OR CAST(Batting_Value_2024.WAR AS NUMERIC) &gt; 0)
--AND (CAST(Batting_2024.AB AS INT) &gt; 200)
--AND Batting_LHP_2024.OPS &gt; .740
--and (Dynasty_March_2024.Rank is not null OR Dynasty_Prospect_March_2024.Rank is not null OR Dynasty_Prospect_March.Rank is not null)
ORDER BY
--Batting_RHP_2024.OPS DESC, 
CAST(Dynasty_November_2024.Rank AS INT) ASC NULLS LAST, 
CAST(December_Prospect_2024.Rank AS INT) ASC NULLS LAST, 
Batting_Value_2024.WAR DESC, Batting_2024.&quot;Player-additional&quot;</sql><sql name="Pitcher-Type Rankings">
SELECT 
pitching.Name,
roster.flb_team_init,
       pitching.Age,
       Dynasty_March_2024.Rank,
--       Dynasty_October.Prospect,
       Dynasty_March_2024.&quot;POS.&quot;,
       Dynasty_March_2024.&quot;SEC.POS.&quot;,
       Dynasty_Prospect_March_2024.Rank,
       Dynasty_Prospect_March_2024.&quot;POS.&quot;,
       Dynasty_Prospect_March_2024.&quot;SEC.POS.&quot;,
       Dynasty_Prospect_March.Rank,
       Dynasty_Prospect_March.&quot;POS.&quot;,
       Dynasty_Prospect_March.&quot;SEC. POS.&quot;,
       pitching.Tm,
       Pitching_Value.WAR,
       pitching.W,
       pitching.L,
       pitching.ERA,
       pitching.G,
       pitching.GS,
       pitching.GF,
       pitching.CG,
       pitching.SHO,
       pitching.SV,
       pitching.IP,
       pitching.H,
       pitching.R,
       pitching.ER,
       pitching.HR,
       pitching.BB,
       pitching.IBB,
       pitching.SO,
       pitching.FIP,
       pitching.WHIP,
       pitching.H9,
       pitching.HR9,
       pitching.BB9,
       pitching.SO9,
       pitching.&quot;SO/W&quot;,
       pitching.&quot;Name-additional&quot;
  FROM pitching
LEFT OUTER JOIN roster on pitching.&quot;Name-additional&quot; = roster.player_id
LEFT OUTER JOIN Pitching_Value on pitching.&quot;Name-additional&quot; = Pitching_Value.&quot;Name-additional&quot;
LEFT OUTER JOIN Fielding on Fielding.&quot;Name-additional&quot; = pitching.&quot;Name-additional&quot;
LEFT OUTER JOIN player_list on player_list.player_id = pitching.&quot;Name-additional&quot;
LEFT OUTER JOIN Dynasty_March_2024 on (player_list.player_name = Dynasty_March_2024.Player OR Dynasty_March_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Dynasty_Prospect_March_2024 on (player_list.player_name = Dynasty_Prospect_March_2024.Player OR Dynasty_Prospect_March_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Dynasty_Prospect_March on (player_list.player_name = Dynasty_Prospect_March.Player OR Dynasty_Prospect_March.Player = player_list.alt_name)
  --inner join fielding on fielding.player_id = pitchers.player_id
 WHERE
  roster.flb_team_init = 'WAIVE'
--  AND (CAST(Pitching.Age AS INT) &lt; 28 OR CAST(Pitching_Value.WAR AS NUMERIC) &gt; 0)
--  AND CAST(pitching.GS AS INT) &gt; 0
--and (Dynasty_March_2024.Rank is not null OR Dynasty_Prospect_March_2024.Rank is not null)
ORDER BY CAST(Dynasty_March_2024.Rank AS INT) ASC NULLS LAST, 
CAST(Dynasty_Prospect_March_2024.Rank AS INT) ASC NULLS LAST, 
CAST(Dynasty_Prospect_March.Rank AS INT) ASC NULLS LAST, 
Pitching_Value.WAR DESC, pitching.&quot;Name-additional&quot;
</sql><sql name="All Pitcher Rankings">SELECT Pitching_2024.Player,
roster.flb_team_init,
       Pitching_2024.Age,
       Dynasty_November_2024.Rank,
       Dynasty_November_2024.&quot;POS.&quot;,
       Dynasty_November_2024.&quot;SEC.POS.&quot;,
       December_Prospect_2024.Rank,
       December_Prospect_2024.&quot;POS.&quot;,
       December_Prospect_2024.&quot;SEC.POS.&quot;,
       December_Prospect_2024.Rank,
       December_Prospect_2024.&quot;POS.&quot;,
       --Dynasty_Prospect_March_2024.&quot;SEC. POS.&quot;,
       Pitching_2024.Team,
       Pitching_Value_2024.WAR,
       Pitching_2024.W,
       Pitching_2024.L,
       Pitching_2024.ERA,
       Pitching_2024.G,
       Pitching_2024.GS,
       Pitching_2024.GF,
       Pitching_2024.CG,
       Pitching_2024.SHO,
       Pitching_2024.SV,
       Pitching_2024.IP,
       Pitching_2024.H,
       Pitching_2024.R,
       Pitching_2024.ER,
       Pitching_2024.HR,
       Pitching_2024.BB,
       Pitching_2024.IBB,
       Pitching_2024.SO,
       Pitching_2024.FIP,
       Pitching_2024.WHIP,
       Pitching_2024.H9,
       Pitching_2024.HR9,
       Pitching_2024.BB9,
       Pitching_2024.SO9,
       Pitching_2024.&quot;SO/BB&quot;,
       Pitching_2024.&quot;Player-additional&quot;
  FROM Pitching_2024
LEFT OUTER JOIN roster on Pitching_2024.&quot;Player-additional&quot; = roster.player_id
LEFT OUTER JOIN Pitching_Value_2024 on pitching_2024.&quot;Player-additional&quot; = Pitching_Value_2024.&quot;Player-additional&quot;
--LEFT OUTER JOIN Fielding on Fielding.&quot;Player-additional&quot; = Pitching_2024.&quot;Player-additional&quot;
LEFT OUTER JOIN player_list on player_list.player_id = Pitching_2024.&quot;Player-additional&quot;
LEFT OUTER JOIN Dynasty_November_2024 on (player_list.player_name = Dynasty_November_2024.Player OR Dynasty_November_2024.Player = player_list.alt_name)
LEFT OUTER JOIN December_Prospect_2024 on (player_list.player_name = December_Prospect_2024.Player OR December_Prospect_2024.Player = player_list.alt_name)
  --inner join fielding on fielding.player_id = pitchers.player_id
 WHERE
 CAST(Pitching_2024.GS AS INT) * 2 &gt;= CAST(Pitching_2024.G AS INT)
 AND    (roster.flb_team_init  IS NULL OR roster.flb_team_init = 'BFP')
