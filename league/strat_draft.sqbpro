a="main" name="player_list" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort><column index="5" mode="1"/></sort><column_widths><column index="1" value="178"/><column index="2" value="98"/><column index="3" value="150"/><column index="4" value="82"/><column index="5" value="122"/><column index="6" value="96"/></column_widths><filter_values><column index="1" value="Antho"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="roster" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort><column index="2" mode="1"/></sort><column_widths><column index="1" value="221"/><column index="2" value="126"/><column index="3" value="97"/></column_widths><filter_values><column index="2" value="WAIVe"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Positional Ranking">SELECT Batting.Name,
--roster.flb_team_init,
       Batting.Age,
       Dynasty_March_2024.Rank,
--       Dynasty_October.Prospect,
       Dynasty_March_2024.&quot;POS.&quot;,
       Dynasty_March_2024.&quot;SEC.POS.&quot;,
       Dynasty_Prospect_March_2024.Rank,
       Dynasty_Prospect_March_2024.&quot;POS.&quot;,
       Dynasty_Prospect_March_2024.&quot;SEC.POS.&quot;,
       Dynasty_Prospect_March.Rank,
       Dynasty_Prospect_March.&quot;POS.&quot;,
       Dynasty_Prospect_March.&quot;SEC. POS.&quot;,
       Batting.Tm,
       Batting_Value.WAR,
       Batting.G,
       Batting.AB,
       Batting.R,
       Batting.H,
       Batting.&quot;2B&quot;,
       Batting.&quot;3B&quot;,
       Batting.HR,
       Batting.RBI,
       Batting.SB,
       Batting.CS,
       Batting.BB,
       Batting.SO,
       Batting.BA,
       Batting.OBP,
       Batting.SLG,
       Batting.OPS,
       Batting.&quot;Pos Summary&quot;,
       --Fielding.&quot;Pos Summary&quot;,
       Batting.&quot;Name-additional&quot;,
	   MainPos
  FROM Batting 
LEFT OUTER JOIN roster on Batting.&quot;Name-additional&quot; = roster.player_id
LEFT OUTER JOIN Batting_Value on Batting.&quot;Name-additional&quot; = Batting_Value.&quot;Name-additional&quot;
LEFT OUTER JOIN player_list on player_list.player_id = Batting.&quot;Name-additional&quot;
LEFT OUTER JOIN Dynasty_March_2024 on (player_list.player_name = Dynasty_March_2024.Player OR Dynasty_March_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Dynasty_Prospect_March_2024 on (player_list.player_name = Dynasty_Prospect_March_2024.Player OR Dynasty_Prospect_March_2024.Player = player_list.alt_name)
LEFT OUTER JOIN Dynasty_Prospect_March on (player_list.player_name = Dynasty_Prospect_March.Player OR Dynasty_Prospect_March.Player = player_list.alt_name)
WHERE 
(
Batting.&quot;Pos Summary&quot; LIKE '%'
--OR Batting.&quot;Pos Summary&quot; LIKE '*7%'
--OR Batting.&quot;Pos Summary&quot; LIKE '/7%'
)
AND roster.flb_team_init = 'WAIVE'
--AND (CAST(Batting.Age AS INT) &lt; 28 OR CAST(Batting_Value.WAR AS NUMERIC) &gt; 0)
ORDER BY 
CAST(Dynasty_March_2024.Rank AS INT) ASC NULLS LAST, 
CAST(Dynasty_Prospect_March_2024.Rank AS INT) ASC NULLS LAST, 
CAST(Dynasty_Prospect_March.Rank AS INT) ASC NULLS LAST, 
Batting_Value.WAR DESC, Batting.&quot;Name-additional&quot;</sql><sql name="All Batter Rankings">SELECT Batting_2025.Player,
roster.flb_team_init,
Batting_2025.Age,
Batting_2025.&quot;Pos&quot;,
       Batting_2025.OPS,
-- 	   Batting_RHP_2024.OPS AS OPS_R,
-- 	   Batting_LHP_2024.OPS AS OPS_L,
-- 	   	   	   Batting_1H_2024.OPS AS OPS_1ST,
-- 	   Batting_2H_2024.OPS AS OPS_2ND,
-- 	   Batting_2H_2024.OPS - Batting_1H_2024.OPS AS OPS_DIFF,
       Dynasty_June_2025.Rank,
       Dynasty_June_2025.&quot;POS.&quot;,
       May_Prospect_2025.Rank,
       May_Prospect_2025.&quot;POS.&quot;,
       BA_Top500_Dynasty.Rank,
       BA_Top500_Dynasty.Position,
       Batting_2025.Team,
       Batting_2025.WAR,
       Batting_2025.G,
       Batting_2025.AB,
       Batting_2025.R,
       Batting_2025.H,
       Batting_2025.&quot;2B&quot;,
       Batting_2025.&quot;3B&quot;,
       Batting_2025.HR,
       Batting_2025.RBI,
       Batting_2025.SB,
       Batting_2025.CS,
       Batting_2025.BB,
       Batting_2025.SO,
       Batting_2025.BA,
       Batting_2025.OBP,
       Batting_2025.SLG,
       Batting_2025.OPS,
       Batting_2025.&quot;Pos&quot;,
       --Fielding.&quot;Pos Summary&quot;,
       Batting_2025.&quot;Player-additional&quot;,
	   MainPos
  FROM Batting_2025 
LEFT OUTER JOIN roster on Batting_2025.&quot;Player-additional&quot; = roster.player_id
LEFT OUTER JOIN player_list on player_list.player_id = Batting_2025.&quot;Player-additional&quot;
LEFT OUTER JOIN Dynasty_June_2025 on (player_list.player_name = Dynasty_June_2025.Player OR Dynasty_June_2025.Player = player_list.alt_name)
LEFT OUTER JOIN May_Prospect_2025 on (player_list.player_name = May_Prospect_2025.Player OR May_Prospect_2025.Player = player_list.alt_name)
--LEFT OUTER JOIN Batting_RHP_2024 on Batting_RHP_2024.&quot;Player-additional&quot; = Batting_2025.&quot;Player-additional&quot;
--LEFT OUTER JOIN Batting_LHP_2024 on Batting_LHP_2024.&quot;Player-additional&quot; = Batting_2025.&quot;Player-additional&quot;
